WelcomeDialog QPushButton#closeButton{
    image: url(:/icons/res/icons/welcome_dialog_close_icon.svg);
    background-color: white; 
    border: none;
    color: black; 
    padding: 10px;
    border-radius: 8px;
}
WelcomeDialog QPushButton#closeButton:hover{
    
    background-color: #ddd; 

}

WelcomeDialog QWidget#listContainerWidget{
    background-color: transparent;
    
}

WelcomeDialog QListView#recentProjectsList{
    border: none;
}



WelcomeDialog QListView {
    background-color: transparent;
    border: none;
    outline: none;
    selection-background-color: #d6d8de;  /* 选中时的背景色 */

    
}

NewProjectDialog QPushButton[style="common"]{
    background-color: #F4F4F4;
    border: 1px solid #BFBFBF;
    border-radius: 4px;
}
NewProjectDialog QPushButton[style="common"]:hover{
    background-color: #c2e4fa;
    border: 1px solid #2f5e9c;
    border-radius: 4px;
}


NewProjectDialog QLineEdit{
    border: 1px solid #BFBFBF;
    border-radius: 4px;
}

