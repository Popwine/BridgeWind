
WelcomeDialog QPushButton[class="welcome_button"]{
    text-align: left;
    background-color: transparent; 
    border: none;
    border-radius: 0;
    color: black; 
    padding-left: 30px;
    padding-right: 30px;

}
WelcomeDialog QPushButton[class="welcome_button"]:hover{
    background-color: #d6d8de; 


}

WelcomeDialog QPushButton#closeButton{
    background-color: #d6d8de; 
    border: none;
    color: black; 

}


WelcomeDialog QDialog {
    background-color: white;

}

WelcomeDialog QWidget#leftWidget {
    background-color: #f5f8ff;
    border-radius: 8px;
}

WelcomeDialog QWidget#welcomeBackgroundWidget {
    background-color: white;
    border-radius: 8px; /* 这里设置你想要的圆角大小 */
    border: 1px solid #102961;
}


