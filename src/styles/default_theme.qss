MainWindow QWidget#backgroundWidget{
    background-color: #e5e5ea;
    border-radius: 8px; /* 这里设置你想要的圆角大小 */
    border: 1px solid #102961;
}

MainWindow QWidget#leftWidget{
    background-color: #e5e5ea;

}
MainWindow QWidget#rightWidget{
    background-color: #f2f2f7;

}
MainWindow QWidget#centralwidget{
    background-color: #e5e5ea;

}

MainWindow QWidget#titleWidget{
    background-color: #e5e5ea;
}


MainWindow QWidget#toggleWidget{
    background-color: #D8D8D8;
    border: none;
    border-radius: 9px;
}


MainWindow QWidget#toggleWidget QPushButton{
    background-color: transparent;


}
MainWindow QWidget#toggleWidget QPushButton:checked{
    background-color: #fff;
    border: none;
    border-radius: 6px;


}


QWidget#toggleGeoModeWidget QWidget#toggleGeoModeButtonWidget QPushButton{
    background-color: transparent;
}
QWidget#toggleGeoModeWidget QWidget#toggleGeoModeButtonWidget QPushButton:checked{
    background-color: transparent;
    color: #0256FF;
}

QWidget#toggleGeoModeWidget QWidget#toggleLineWidget QPushButton{
    background-color: #D8D8D8;
    border-radius: 0px;
}
QWidget#toggleGeoModeWidget QWidget#toggleLineWidget QPushButton:checked{
    background-color: #0256FF;

}

QStackedWidget#parameterStack QWidget{
    background-color: transparent;
    border: none;
    
}
QStackedWidget#parameterStack QLineEdit{

    background-color: white;
    border: 1px solid rgba(0, 0, 0, 0.2);
    border-radius: 3px;
}

